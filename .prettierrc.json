{
	"overrides": [
		{
			"files": "*.sol",
			"options": {
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": true,
				"singleQuote": false,
				"bracketSpacing": false
			}
		},
		{
			"files": ["*.ts", "*.js", "*.json"],
			"options": {
				"printWidth": 100,
				"tabWidth": 4,
				"useTabs": true,
				"singleQuote": false
			}
		}
	]
}
